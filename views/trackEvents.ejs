<!DOCTYPE html>
<!-- Created By CodingNepal - www.codingnepalweb.com -->
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>ALL COURSES</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/trackEvents.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"/>
    <script src="https://kit.fontawesome.com/9d595d1bf3.js" crossorigin="anonymous"></script>
  </head>
  <body>
    <nav>
        <label class="logo">SportScheduler</label>
          <li class="link"><a class="anc" href="admindashBoard">Home</a></li>
          <li class="link"><a class="anc" href="AllEvents">Events</a></li>
          <li class="link"><a class="anc" href="addEvent">Add Event</a></li>
          <li class="link"><a class="anc" href="trackEvent">trackEvents</a></li>
          <li class="link"><a class="anc" href="#">Feedback</a></li>
          <li class="link"><a class="anc"  href="#">About</a></li>
          <li class="profile">
          <i class="fa-regular fa-user"></i>
      </nav>
    <section class="event-section">
        <h1>Registration</h1>
        <ul class="event-list">
          <% formattedData.forEach(user => { %>
            <div class="event-item"> 
                <li >User: <%= user.firstName %></li>
                <li> Email: <%= user.email %></li>
            </div>
            </ul>
          <% }); %>
        </ul>
      </section>
      
      <script>
        document.addEventListener('DOMContentLoaded', function () {
          const currentPage = window.location.href;
    
          const links = document.querySelectorAll('.anc');
    
          links.forEach(link => {
            const linkHref = link.getAttribute('href');
            if (currentPage.includes(linkHref)) {
              link.classList.add('active');
            }
          });
        });
      </script>

  </body>
</html>